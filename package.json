{
  "name": "dad-jokes",
  "version": "1.0.0",
  "description": "A npm wrapper for the dadjokes.io apie",
  "main": "index.js",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DadJokes-io/DadJokes-npm.git"
  },
  "keywords": [
    "Dad",
    "Jokes",
    "Funny",
    "API"
  ],
  "author": "Kegen Guyll",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DadJokes-io/DadJokes-npm/issues"
  },
  "homepage": "https://github.com/DadJokes-io/DadJokes-npm#readme",
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "jest": "^27.0.4",
    "prettier": "^2.3.1",
    "ts-jest": "^27.0.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.3.2"
  },
  "files": [
    "lib/**/*"
  ]
}
